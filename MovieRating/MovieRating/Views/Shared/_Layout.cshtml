<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/Scripts/Kendo UI for jQuery R3 2020/js/jquery.min.js"></script>
    <script src="~/Scripts/Kendo UI for jQuery R3 2020/js/kendo.all.min.js"></script>
    <script src="~/Scripts/Kendo UI for jQuery R3 2020/js/kendo.grid.min.js"></script>

    <link href="~/Scripts/Kendo UI for jQuery R3 2020/styles/kendo.blueopal.min.css" rel="stylesheet" />
    <link href="~/Scripts/Kendo UI for jQuery R3 2020/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="~/Scripts/Kendo UI for jQuery R3 2020/styles/kendo.bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Movie 1", "Index", "Home")</li>
                    <li>@Html.ActionLink("Movie 2", "Index", "Home")</li>
                    <li>@Html.ActionLink("Movie 3", "Index", "Home")</li>
                    <li>@Html.ActionLink("Movie 4", "Index", "Home")</li>
                    <li>@Html.ActionLink("Movie 5", "Index", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="movie body-content">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

